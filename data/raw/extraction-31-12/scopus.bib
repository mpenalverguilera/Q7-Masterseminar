@CONFERENCE{Song20232986,
	title = {LLM-Planner: Few-Shot Grounded Planning for Embodied Agents with Large Language Models},
	doi = {10.1109/ICCV51070.2023.00280},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85188253116&doi=10.1109%2fICCV51070.2023.00280&partnerID=40&md5=9f5c02d573d45bed9c0c70a9eba5cea4},
	abstract = {This study focuses on using large language models (LLMs) as a planner for embodied agents that can follow natural language instructions to complete complex tasks in a visually-perceived environment. The high data cost and poor sample efficiency of existing methods hinders the development of versatile agents that are capable of many tasks and can learn new tasks quickly. In this work, we propose a novel method, LLM-Planner, that harnesses the power of large language models to do few-shot planning for embodied agents. We further propose a simple but effective way to enhance LLMs with physical grounding to generate and update plans that are grounded in the current environment. Experiments on the ALFRED dataset show that our method can achieve very competitive few-shot performance: Despite using less than 0.5% of paired training data, LLM-Planner achieves competitive performance with recent baselines that are trained using the full training data. Existing methods can barely complete any task successfully under the same few-shot setting. Our work opens the door for developing versatile and sample-efficient embodied agents that can quickly learn many tasks. © 2023 IEEE.}
}

